<% @places.each do |place| %>
	<div class="booyah-box col-10 offset-1">
		<h1><%= place.name %></h1>
		<i><%= place.address %></i><br /><br />
		<p><%= place.description %></p><br />
	</div>
<% end%>

	<%= will_paginate @places, renderer: BootstrapPagination::Rails, previous_label: "Back", next_label: "Next", inner_window: 1, outer_window: 0 %>